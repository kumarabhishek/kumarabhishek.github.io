{
  "name": "react-equalizer",
  "version": "1.0.0",
  "engines": {
    "node": "9.3.0",
    "npm": "5.6.0"
  },
  "description": "Demo project to showcase Express.js + React.js + mongodb based CRUD web application.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "predev": "npm run build:prd && babel client/src --out-dir client/build --copy-files --ignore index.html --minified --source-maps",
    "dev": "npm run serve",
    "serve": "node server dev",
    "build:prd": "webpack --config client/config/webpack/webpack.config.babel.js --env prd -p",
    "prebuild": "rimraf client/build/*",
    "build": "npm run build:prd",
    "postbuild": "babel client/src --out-dir client/build --copy-files --ignore index.html --minified --source-maps && npm run serve:prd",
    "serve:prd": "node server"
  },
  "keywords": [
    "react.js",
    "equalizer",
    "express.js",
    "ui",
    "es6"
  ],
  "author": "Kumar Abhishek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kumarabhishek/kumarabhishek.github.io/issues"
  },
  "homepage": "https://github.com/kumarabhishek/kumarabhishek.github.io/tree/master/apps/equalizer",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.15.0",
    "express": "^5.0.0-alpha.5",
    "html-webpack-plugin": "^2.30.1",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.2",
    "react-router-dom": "^4.2.2",
    "rimraf": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.0"
  }
}
